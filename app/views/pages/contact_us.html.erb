  				<option value="service">General Customer Service</option>
  				<option value="suggestions">Suggestions</option>
  				<option value="product">Product Support</option>
  			</select>
      
         <label>How did you hear about us?</label>
         <select id="advertising" name="advertising" class="span10 ">
          <option value="referral" selected="">Another Doctor</option>
          <option value="google">Google Ads</option>
          <option value="internet">Other places on the internet</option>
          <option value="suggestions">Suggestions</option>
          <option value="product">Product Support</option>
          <option value="jobs">Career Enquiry</option>
        </select>
      </div>

      <div class="span6">
			<label>Comments</label>
			<textarea name="message" id="message" class="input-xlarge span10" rows="12">
      </textarea>
      <br>
  		<button type="submit" class="btn btn-custom-darken text-center btn-large offset3 ">Send</button>
    </div>
     


  </form>
